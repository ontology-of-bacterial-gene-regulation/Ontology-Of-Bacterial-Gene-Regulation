    <!-- http://purl.obolibrary.org/obo/PROK_0000193 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PROK_0000193">
        <obo:IAO_0000115>A realizable property that inheres in a transcription factor (TF), by means of which the TF is capable of regulating gene expression under appropriate conditions. The realization of this disposition occurs when the TF binds its target DNA and modulates transcription.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">TF functional disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PROK_0000194 -->

